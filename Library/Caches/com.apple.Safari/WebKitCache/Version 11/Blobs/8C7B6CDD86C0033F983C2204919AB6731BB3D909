// SEARCH INPUT
jQuery.widget("custom.autocomplete", jQuery.ui.autocomplete, {
 _renderMenu: function (ul, items) {
var that = this,
 currentCategory = "";
jQuery.each(items, function (index, item) {
if (item._type != currentCategory) {
 ul.append("<h3 class='ui-autocomplete-category search_title'>" + (item._type == "serie" ? 'Series TV' : '') + (item._type == "movies" ? 'Peliculas' : '') + (item._type == "kids" ? 'Niños' : '') + "</h3>");
 currentCategory = item._type;
}
that._renderItemData(ul, item);
});
 }
});
jQuery(function () {
jQuery("[name=s]").autocomplete({
appendTo: ".search",
delay: 0,
source: function (request, response) {
jQuery(".ui-autocomplete").addClass("searchLoading");
jQuery(".ui-autocomplete").empty();
jQuery(".ui-autocomplete").show();
jQuery.ajax({
 type: "get",
 dataType: "json",
 data: {"q": request.term},
 success: function (data) {
//console.log(data);
response(jQuery.map(data.hits.hits, function (item) {
jQuery(".ui-autocomplete").removeClass("searchLoading");
return item;
}));
 }
});
},
minLength: 3,
focus: function (event, ui) {
event.preventDefault(); // without this: keyboard movements reset the input to ''
jQuery(this).val(jQuery('.ui-state-focus hgroup h2').html());
},
 }).data('customAutocomplete')._renderItem = function (ul, item) {
var urls = [];
urls["kids"] = "http://www.pelispedia.tv/pelicula/";
urls["movies"] = "http://www.pelispedia.tv/pelicula/";
urls["serie"] = "/show/";
return $("<li class='" + item._type + "'></li>")
.data("ui-autocomplete-item", item)
.append("<a href='"+urls[item._type]+item._id+"'>" + "<figure class='search_thumb_container'><img src='" + (typeof (item._source.thumbnail) != "undefined" ? '' + item._source.thumbnail + '' : 'http://placehold.it/52x77') + "' /></figure><hgroup><h2>" + (item._type == "live" ? '' + item._source.eventName + '' : '' + item._source.title + '') + '</h2>' + (item._type == "movie" ? '<h3>(' + (typeof (item._source.year) != "undefined" ? '' + item._source.year + '' : '') + ')</h3>' : '') + '<h5>' + (typeof (item._source.genres) != "undefined" ? '' + (item._source.genres).toString().replace(/\s*,\s*|\s+,/g, ', ') + '' : '') + '' + (typeof (item._source.genre) != "undefined" ? '' + (item._source.genre).replace('-', ' ') + '' : '') + (typeof (item._source.tuneInDate) != "undefined" ? '' + (item._source.tuneInDate) + '' : '') + (typeof (item._source.tuneInHour) != "undefined" ? ' - ' + (item._source.tuneInHour) + '' : '') + '</h5></hgroup></a>')
.appendTo(ul);
 };
}); //End Funcion

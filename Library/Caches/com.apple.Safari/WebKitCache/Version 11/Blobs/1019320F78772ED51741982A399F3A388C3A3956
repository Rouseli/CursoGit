<script>
			jQuery(function(){

				var loadEmbedOptions = function(optString){
					try {
					    return JSON.parse(optString);
					}
					catch(err) {
					    console.log('Embed options were not set', err)
					    return 0;
					}
				}

				var validateSource = function(url){
					return /^(?:(?:(?:https?|ftp):)?\/\/)(?:\S+(?::\S*)?@)?(?:(?!(?:10|127)(?:\.\d{1,3}){3})(?!(?:169\.254|192\.168)(?:\.\d{1,3}){2})(?!172\.(?:1[6-9]|2\d|3[0-1])(?:\.\d{1,3}){2})(?:[1-9]\d?|1\d\d|2[01]\d|22[0-3])(?:\.(?:1?\d{1,2}|2[0-4]\d|25[0-5])){2}(?:\.(?:[1-9]\d?|1\d\d|2[0-4]\d|25[0-4]))|(?:(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)(?:\.(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)*(?:\.(?:[a-z\u00a1-\uffff]{2,})).?)(?::\d{2,5})?(?:[/?#]\S*)?$/i.test( url );
				}

				var winName = window.name,
					embedOpts = loadEmbedOptions(winName);

				/*
					options = 
					{
						srt : [ { label: '', source: '' }, { name: '', source: '' } ]
						autoplay : true|false
					}
				*/
				if(embedOpts != 0){
					console.log('embedOpts: ',embedOpts);
					
					if( typeof embedOpts.src != "undefined" ){
						var i;
						for( i in embedOpts.src ){
							if( !validateSource(embedOpts.src[i].source) || embedOpts.src[i].label == "" ) continue;
							console.log('tracks', jwConfig_vars.playlist[0].tracks);
							jwConfig_vars.playlist[0].tracks.push({
								"kind": "captions",
								"file": embedOpts.src[i].source,
								"label": embedOpts.src[i].label
							});
						}	
					}
					if( typeof embedOpts.autostart != "undefined" ){
						jwConfig_vars.autostart = (embedOpts.autostart)? "true": "false";
					}
					theVideoPlayer.setup(jwConfig(jwConfig_vars));
					console.log('redid the player', jwConfig_vars);
				}
			});


			</script>

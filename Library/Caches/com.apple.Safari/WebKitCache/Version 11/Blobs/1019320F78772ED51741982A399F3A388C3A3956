    var theVideoPlayer = jwplayer("vplayer");
    theVideoPlayer.setup(jwConfig(jwConfig_vars));

    theVideoPlayer.on('ready', postSetup);
    var resumeVideoTime = 0,
    videoResumedFlag = false;

    function postSetup() {
      // player's main container
      var playerContainer = theVideoPlayer.getContainer();
      // our separator between btns in controlbar
      var separator = $('<span> </span>')
        .css('border-left','1px solid #666')
        .css('float','left')
        .css('height','15.5px')
        .css('margin-top','7px')
        .css('content', ' ');
      // place logo in controlbar
      var logoToolTip = $('<div></div>')
        .addClass('player-tooltip')
        .html('Watch on TheVideo');
      var logoDiv = $('<a></a>')
        .addClass('jw-icon jw-icon-inline jw-button-color jw-reset jw-mainlogo')
        .css('background-image', 'url('+theVideoPlayer.getConfig().logo.mainLogo+')')
        .append(separator)
        .append(logoToolTip)
        .attr('href', theVideoPlayer.getConfig().logo.link)
        .attr('target', theVideoPlayer.getConfig().logo.target);
      if(document.location.pathname.match('/embed')) $(playerContainer).find('.jw-controlbar-right-group .jw-icon-fullscreen').before(logoDiv);
      // place download btn in controlbar
      var downloadBtnToolTip = $('<div></div>')
        .addClass('player-tooltip')
        .html('Download');
      var downloadBtn = $('<a></a>')
        .addClass('jw-icon jw-icon-custom jw-icon-inline jw-button-color jw-reset jw-btn-download jw-icon-cloud-download')
        .append(downloadBtnToolTip)
        .attr('href', 'https://thevideo.me/download/'+theVideoPlayer.getPlaylistItem().mediaid)
        .attr('target', '_blank');
      $(playerContainer).find('.jw-controlbar-right-group .jw-mainlogo').before(downloadBtn);
      // change share btn to hover and say "share" 
      // little hack to put a 1m second delay to assure the element exists
      setTimeout(function(){ $(".jwplayer div[button='share'] .jw-dock-text").html('Share'); }, 100); 

      // add player mode toggle
      function thevideo_playermode() {
      // console.log('WRITING PLAYER MODE BTN', mode);

      btnObj = {}
      if(theVideoPlayer.getProvider().name == 'html5') {
        btnObj.img = 'https://thevideo.me/player/icons/mode_flash_white.png';
        btnObj.txt = 'Flash';
      }else{
        btnObj.img = 'https://thevideo.me/player/icons/mode_html5_white.png';
        btnObj.txt = 'HTML5';
      }

      theVideoPlayer.addButton(btnObj.img,
        "Switch to "+btnObj.txt+" Player",
        function() {
          var mode_to_set,
          current_mode = theVideoPlayer.getProvider().name,
          mode_name;
          if ( current_mode == 'html5') {
            // switch to flash player
            mode_to_set = 'flash';
            mode_name = 'Flash';
          }else{
            mode_to_set = 'html5';
            mode_name = 'HTML5';
          }
          $.cookie('player_mode', mode_to_set, { expires: 365 });
          jwConfig_vars.primary = mode_to_set;
          resumeVideoTime = theVideoPlayer.getPosition();
          videoResumedFlag = false;
          theVideoPlayer.setup(jwConfig(jwConfig_vars));
          theVideoPlayer.on('ready', function() { resumeVideo(); postSetup(); });
          // thevideo_playermode(mode_name);
        },
        "player_mode" );
    }
    thevideo_playermode();
    thevideo.playersrt.init();    
  }

  function resumeVideo() {
    console.log('video resume called', resumeVideoTime);
    if(resumeVideoTime) {
      theVideoPlayer.play(true);
      console.log('video played');
      theVideoPlayer.on('play', function() {
        console.log('on play detected, seeking the video');
        if(!videoResumedFlag) {
          theVideoPlayer.seek(resumeVideoTime);
          videoResumedFlag = true;
        }
      });
    }
  }

        if (typeof queryDict["member"] !== 'undefined') {
            var getTheVar = newAdvCookie.substr(0, newAdvCookie.indexOf(","));
            newAdvCookie = newAdvCookie.replace(getTheVar,queryDict["member"]);
        }
        if (typeof queryDict["var1"] !== 'undefined') {
            if (newAdvCookie.indexOf("var1") !== -1) {
                if (newAdvCookie.indexOf("var1:;") !== -1) {
                    //we have it, and it's empty
                    newAdvCookie = newAdvCookie.replace("var1:;","var1:"+queryDict["var1"])+";";
                } else {
                    //we have it, but not empty
                    var getTheVar = newAdvCookie.substr(newAdvCookie.indexOf("var1:") + 5, newAdvCookie.length);
                    getTheVar = getTheVar.substr(0, getTheVar.indexOf(";"));
                    newAdvCookie = newAdvCookie.replace(getTheVar,queryDict["var1"]);
                }
            } else {
                if (newAdvCookie.slice(-1) === ";" || newAdvCookie.slice(-1) === ","){
                    newAdvCookie = newAdvCookie + "var1:" + queryDict["var1"]+";";
                } else {
                    newAdvCookie = newAdvCookie + ";var1:" + queryDict["var1"]+";";
                }
            }
        }
        if (typeof queryDict["var2"] !== 'undefined') {
            if (newAdvCookie.indexOf("var2") !== -1) {

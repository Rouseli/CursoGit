        //             }
        //         });
        //
        //     };
        // }

    },
    init: function(alertsData, behavior, lang) {

        window.removeExitPopup = function() {
            window.exitPopupUnload = false;
            window.alertManager.abstractPopUp.enabled(false);
        }

        if (arguments.length) {
            this.translates = alertsData || this.translates || alertsTranslates;
            this.behavior = behavior || this.behavior || 0;
            this.lang = lang || this.lang || this.defaultLang;

        }

        if (!this.initStage) { // first run

            this.initStage = 1;

            if (!/loaded|complete/.test(document.readyState)) { // wait till load, then return

                if (window.addEventListener) { // W3C standard
                    window.addEventListener('load', alertManagerInit, false); // NB **not** 'onload'
                } else if (window.attachEvent) { // Microsoft
                    window.attachEvent('onload', alertManagerInit);
                }

                return;
            }

        }

        if (this.initStage === 1 && /loaded|complete/.test(document.readyState)) { // load complete, wait till render

            this.initStage = 2;

            window.setTimeout('alertManager.initStage = 3;alertManagerInit();', 200);

            return;
        }

        if (this.initStage === 3) { // render complete, init

            this.initStage = 4;

            if (window.lang_param) { // global parameter

                this.lang = window.lang_param; // can be changed while page load
            }

            this.abstractPopUp.setManager(this);

            for (var i in this.popUpsCollection) {
                var popUpConstructor = this.popUpsCollection[i];
                popUpConstructor.prototype = this.abstractPopUp;
            }

            var popUpsIds = this.getTypes(this.behavior);

            var result = null;
            for (var i in popUpsIds) {
                var popUpId = popUpsIds[i];
                if (popUpId) {
                    var popUp = this.getPopUp(popUpId);
                    if (popUp && popUp.enabled()) {
                        result = popUp.run(result);
                    }
                }
            }

        } else {


        }

    },
    getPopUp: function(type, event) {
        if (!type || ('function' !== typeof (this.popUpsCollection[type]))) {
            return null;
        }
        return (new this.popUpsCollection[type]);
    },
    getText: function(name) {
        if (undefined === this.translates[name]) {
            return [];
        }
        var translates = this.translates[name];

        return (undefined !== translates[this.lang])
                ? translates[this.lang]
                : translates[this.defaultLang];
    },
    getTypes: function(behavior) {
        if (undefined === this.behaviors[behavior]) {
            return this.behaviors[this.defaultBehavior];
        }
        return this.behaviors[behavior];
    },
    toUpperCase: function(data) {
        if (typeof (data) === 'object') {
            for (var k in data) {
                data[k] = data[k].toUpperCase();
            }
        } else {
            data = data.toUpperCase();
        }

        return data;
    },
    cookie: function(name, value, options) {
        if (typeof value != 'undefined') { // name and value given, set cookie
            options = options || {};
            if (value === null) {
                value = '';
                options.expires = -1;
            }
            var expires = '';
            if (options.expires && (typeof options.expires == 'number' || options.expires.toUTCString)) {
                var date;
                if (typeof options.expires == 'number') {
                    date = new Date();
                    date.setTime(date.getTime() + (options.expires * 24 * 60 * 60 * 1000));
                } else {
                    date = options.expires;
                }
                expires = '; expires=' + date.toUTCString(); // use expires attribute, max-age is not supported by IE
            }
            // CAUTION: Needed to parenthesize options.path and options.domain
            // in the following expressions, otherwise they evaluate to undefined
            // in the packed version for some reason...
            var path = options.path ? '; path=' + (options.path) : '';
            var domain = options.domain ? '; domain=' + (options.domain) : '';
            var secure = options.secure ? '; secure' : '';
            document.cookie = [name, '=', encodeURIComponent(value), expires, path, domain, secure].join('');
        } else { // only name given, get cookie
            var cookieValue = null;
            if (document.cookie && document.cookie != '') {
                var cookies = document.cookie.split(';');
                for (var i = 0; i < cookies.length; i++) {
                    var cookie = jQuery.trim(cookies[i]);
                    // Does this cookie string begin with the name we want?
                    if (cookie.substring(0, name.length + 1) == (name + '=')) {
                        cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                        break;
                    }
                }
            }
            return cookieValue;
        }
    },
    getBrowser: function() {
        var ua = navigator.userAgent.toLowerCase();

        return !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0 ? 'safari' : //for opera safari
        ua.indexOf('chrome') > -1 ? 'chrome' :
            ua.indexOf('mozilla') > -1 && ua.indexOf('safari') < 0 && ua.indexOf('compatible') < 0 ? 'firefox' :
                'safari';
    }
};

function alertManagerInit() {

    alertManager.init();
}

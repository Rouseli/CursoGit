                            $body.find('.js-download:first').trigger('click');
                            popUpExit.fadeOut(300);
                        });
                        $('.js-download').click(function() {
                            $('.js-exitPopup__trigger').hide();
                        });
                    }
                });
            };
        },
        53: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser();
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/exitpopupclone27/index.php?lang=' + self.manager.lang, function(data) {
                    var popUp,
                        $body = $(document.body),
                        classOpen = 'popup_dialog-open';
                    $body.append(data);
                    popUp = $('#js-popup_dialog-exit');

                    var link = $body.find('.js-download').attr('href');
                    popUp.find('#startDownload').attr('href', link);
                    popUp.find('#startDownload2').attr('href', link);

                    var showed = false;

                    if (popUp.length) {

                        $body.find('.js-exitPopup__trigger')
                            .bind('mouseenter', function (event) {
                                posY = event.clientY;
                            })

                            .bind('mouseleave', function (event) {
                                if (event.clientY < posY && !showed) {
                                    popUp .fadeIn(200);
                                    showed = true;
                                }
                            });
                        $('.js-exitPopup__close').click(function() {
                            popUp.fadeOut(300);
                        });
                        $('#startDownload').click(function(){
                            $body.find('.js-download:first').trigger('click');
                            popUp.fadeOut(300);
                        });
                        $('#startDownload2').click(function(){
                            $body.find('.js-download:first').trigger('click');
                            popUp.fadeOut(300);
                        });

                        $('.download, .js-download').click(function() {
                            $('.js-exitPopup__trigger').hide();
                        });
                    }
                });

            };
        }
        // 53: function() {
        //     this.name = 'lending_consider_popup';
        //     this.enabled = this.enabledWithShowed;
        //     this.run = function() {
        //         var self = this, browser = alertManager.getBrowser();

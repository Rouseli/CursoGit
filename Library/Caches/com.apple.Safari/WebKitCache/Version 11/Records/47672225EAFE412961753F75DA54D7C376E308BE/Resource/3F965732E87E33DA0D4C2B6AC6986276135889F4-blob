        26: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser();
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/exitpopup/index.php?lang=' + self.manager.lang, function(data) {
                    var popUpExit,
                        $body = $(document.body),
                        classOpen = 'popup_dialog-open';
                    $body.append(data);
                    popUpExit = $('#js-popup_dialog-exit');

                    var link = $body.find('.js-download').attr('href');
                    popUpExit.find('#startDownload').attr('href', link);

                    var showed = false;

                    if (popUpExit) {

                        $body.find('.js-exitPopup__trigger')
                            .bind('mouseenter', function (event) {
                                posY = event.clientY;
                            })

                            .bind('mouseleave', function (event) {
                                if (event.clientY < posY && !showed) {
                                    popUpExit .fadeIn(200);
                                    showed = true;
                                }
                            });
                        $('.js-exitPopup__close').click(function() {
                            popUpExit.fadeOut(300);

                        });
                        $('#startDownload').bind('click', function(){
                            $body.find('.js-download:first').trigger('click');
                            popUpExit.fadeOut(300);
                        });
                        $('.js-download').click(function() {
                            $('.js-exitPopup__trigger').hide();
                        });
                    }
                });
            };
        },

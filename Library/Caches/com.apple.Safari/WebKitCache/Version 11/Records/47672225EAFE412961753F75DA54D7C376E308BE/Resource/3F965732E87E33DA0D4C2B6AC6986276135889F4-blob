window.alertManager = window.alertManager || {
    translates: null,
    defaultBehavior: 0,
    defaultLang: 'en',
    commomCookieName: 'popupShowed',
    // behavior: [popUpId_1, ... ,popUpId_n]
    behaviors: {
        0:  [],
        1:  [1],
        2:  [2],
        3:  [1, 2],
        7:  [10, 6],
        10: [6],
        11: [7],
        12: [9],
        13: [5],
        14: [10],
        15: [11],
        17: [17],
        18: [18],
        19: [19],
        20: [20],
        21: [21],
        22: [22],
        23: [23],
        24: [24],
        25: [25],
        26: [26],
        27: [27],
        28: [28],
        29: [29],
        30: [30],
        31: [31],
        32: [32],
        33: [10, 26],
        34: [10, 27],
        35: [18, 26],
        36: [18, 27],
        37: [37],
        38: [38],
        39: [39],
        40: [40],
        41: [41],
        42: [42],
        43: [43],
        44: [40, 2],
        45: [41, 2],
        46: [10, 6],
        47: [47],
        52: [52],
        53: [53]
    },
    abstractPopUp: {
        isEnabled: true,
        isShowed: false,
        event: null,
        name: null,
        manager: null,
        delimeter: "\n",
        setManager: function(manager) {
            this.manager = manager;
            return this;
        },
        getName: function() {
            return this.name;
        },
        getText: function() {
            return this.manager.getText(this.getName()).join(this.delimeter);
        },
        enabled: function(val) {
            if (undefined !== val) {
                this.isEnabled = val;
            }
            return this.isEnabled;
        },
        showed: function(val) {
            if (undefined !== val) {
                this.isShowed = val;
            }
            return this.isShowed;
        },
        run: function(data) {
            if (!this.enabled()) {
                return null;
            }
            var result = confirm(this.getText());
            this.showed(result);
            return result;
        },
        registerOnBeforeUnload: function(result, popUp) {
            var self = popUp || this;
            $('a,button,input,select').bind('click', function(e) {
                var target = $(this).attr('target');
                if (!target || target.toLowerCase() === '_self') {
                    self.enabled(false);
                }
            });
            $('select').bind('mouseover', function(e) {
                self.enabled(false);
            }).bind('mouseout', function(e) {
                self.enabled(true);
            });

            window.onbeforeunload = function(e) {
                if (!self.enabled()) {
                    return null;
                }
                self.showed(true);
                var str = self.getText();
                var e = e || window.event;
                // For IE and Firefox
                if (e) {
                    e.returnValue = str;
                }
                // For Safari
                return str;
            };
        },
        // reuseful additional functions
        showedOnce: function(val) {
            if (undefined !== val) {
                alertManager.cookie(this.cookieName, 1, {expires: 1});
            }
            return alertManager.cookie(this.cookieName);
        },
        showedWhileCancel: function(val) {
            if (undefined !== val && val) {
                alertManager.cookie(this.cookieName, 1 * val, {expires: 1});
            }
            return alertManager.cookie(this.cookieName);
        },
        enabledWithShowed: function(val) {
            this.parentEnabled = Object.getPrototypeOf(this).enabled;
            this.parentEnabled(val);
            return this.isEnabled && !this.showed();
        }
    },
    // type: constructor
    popUpsCollection: {
        // classic onLoad. Show once on common cookie
        1: function() {
            this.name = 'lending_consider_popup';
            this.cookieName = this.manager.commomCookieName;
            this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
        },
        // classic on Close. Persistent
        2: function() {
            this.name = 'lending_mcprotection_4free';
            this.run = function(result) {
                this.registerOnBeforeUnload(result);
            }
        },
        // onLoad. if cancel show again
        3: function() {
            this.name = 'lending_consider_popup';
            this.cookieName = 'popup3';
            this.showed = this.showedWhileCancel;
            this.enabled = this.enabledWithShowed;
        },
        // classic on Close. Only once
        4: function() {
            this.name = 'lending_mcprotection_4free';
            this.cookieName = 'popup4';
            this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
            this.run = function(result) {
                this.registerOnBeforeUnload(result);
            }
        },
        // like 1. if press 'ok' go to postdownload
        5: function() {
            var firstPopUp = this.manager.getPopUp(1);

            for (var p in firstPopUp) {
                this[p] = firstPopUp[p];
            }

            this.cookieName = 'popup5';

            this.run = function() {
                this.run = Object.getPrototypeOf(this).run;
                if (this.run()) {
                    var downloadAnchor = $('.download, .js-download');
                    if (!downloadAnchor.length) {
                        downloadAnchor = $('a.btn[href*="/download"]');
                    }
                    if (downloadAnchor.length) {
                        downloadAnchor.parent().find('a')[0].click();
                    } else {
                    }
                }
            };
        },
        // onClose. Text from classic onLoad, but to upper case. Persistent
        6: function() {
            this.name = 'lending_consider_popup';
            this.getText = function() {
                this.parentGetText = Object.getPrototypeOf(this).getText;
                return this.manager.toUpperCase(
                        this.parentGetText()
                        );
            };
            this.run = function(result) {
                this.registerOnBeforeUnload(result);
            }
        },
        // onLoad. Init popUp by click in any free place
        7: function() {
            this.name = 'lending_consider_popup';
            this.cookieName = 'popup7';
            this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                $('a,button,input,select,.js-imgslider__btn').click(function(event) {
                    event.stopPropagation();
                });
                var self = this;
                $(document.body).click(function() {
                    if (!self.enabled()) {
                        return null;
                    }
                    confirm(self.getText());
                    self.showed(1);
                });
            };
        },
        // onClose. Text from classic onLoad, but to upper case. Once
        8: function() {
            this.name = 'lending_consider_popup';
            this.cookieName = 'popup8';
            this.getText = function() {
                this.parentGetText = Object.getPrototypeOf(this).getText;
                return this.manager.toUpperCase(
                        this.parentGetText()
                        );
            };
            this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
            this.run = function(result) {
                this.registerOnBeforeUnload(result);
            }
        },
        // onLoad. Custom pop-up
        9: function() {
            this.name = 'lending_consider_popup';
            this.cookieName = 'popup9';
            this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this;
                $.get(window.location.protocol + '//' + window.location.hostname + "/landings/libs/alert/alerts/safari/index.html", function(data) {
                    $(document.body).append(data);
                    var popUp = $('#js-popup_dialog');
                    popUp .fadeIn(700);
                    popUp.find('.text').text(self.getText());
                    popUp.find('button').click(function() {
                        popUp.fadeOut(300);
                    });
                    self.showed(1);
                });
            };
        },
        10: function() {
            this.name = 'lending_consider_popup_compliance';
            this.cookieName = 'popup10';
            this.delimeter = "<br>";
            //this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser(),
                    subPath = (browser === 'safari' ? 'safari_2': browser);

                $.get(window.location.protocol + '//' + window.location.hostname + "/landings/libs/alert/alerts/" + subPath + "/index.html", function(data) {
                    var $body = $(document.body),
                        classOpen = 'popup_dialog-open';
                    $body.addClass(classOpen).append(data);
                    var popUp = $('#js-popup_dialog, #popup_dialog');
                    popUp.fadeIn(700);
                    popUp.find('.text').html(self.getText());
                    popUp.find('button').click(function() {
                        var downloadAnchor;

                        if ($(this).hasClass('ok')) {
                            downloadAnchor = $('.download, .js-download');
                            if (!downloadAnchor.length) {
                                downloadAnchor = $('a[href*="/download"]');
                            }
                            if (downloadAnchor.length) {
                                var target = downloadAnchor.parent().find('a')[0];

                                if(target.click){
                                    target.click();

                                 //for Safari 5.1.10
                                }else if(document.createEvent) {
                                    var eventObj = document.createEvent('MouseEvents');
                                    eventObj.initEvent('click',true,true);
                                    target.dispatchEvent(eventObj);
                                }
                            } else {
                            }
                        }
                        $body.removeClass(classOpen);
                        popUp.fadeOut(300);
                    });

                    self.showed(1);
                });
            };
        },
        11: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this;
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/safari/index.html', function(data) {

                    $(document.body).append(data);
                    var popUp = $('#js-popup_dialog');
                    if (popUp.length) {
                        popUp .fadeIn(700);

                        popUp.find('.text').text(self.getText());

                        popUp.find('button').click(function() {
                            popUp.fadeOut(300);
                        });

                        self.showed(1);
                    }
                });
            };
        },
        //при отмене редирект на страницу http://mackeeper.com/
        12: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                this.run = Object.getPrototypeOf(this).run;
                if (!this.run()) {
                    // var search = window.location.search.replace('alert=', 'exalert=');
                    // window.location.assign('http://mackeeperapp.com/landings/81/' + search);
                    window.location.assign('http://mackeeper.com/');
                }
            };
        },
        17: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this;

                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/safari/index.html', function(data) {

                    $(document.body).append(data);
                    var popUp = $('#js-popup_dialog');
                    if (popUp.length) {
                        popUp .fadeIn(700);

                        popUp.find('.text').text(self.getText());

                        popUp.find('button').click(function() {

                            if ($(this).hasClass('ok')) {
                                var downloadAnchor = $('.download, .js-download');
                                if (!downloadAnchor.length) {
                                    downloadAnchor = $('a.btn[href*="/download"]');
                                }
                                if (downloadAnchor.length) {
                                    var target = downloadAnchor.parent().find('a')[0];

                                    if(target.click){
                                        target.click();

                                        //for Safari 5.1.10
                                    }else if(document.createEvent) {
                                        var eventObj = document.createEvent('MouseEvents');
                                        eventObj.initEvent('click',true,true);
                                        target.dispatchEvent(eventObj);
                                    }
                                } else {
                                }
                            }
                            popUp.fadeOut(300);
                            return false;
                        });

                        self.showed(1);
                    }
                });
            };
        },
        18: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser(),
                    subPath = (browser === 'safari' ? 'safari_2': browser);
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/' + subPath + '/index.html', function(data) {
                    var popUp,
                        $body = $(document.body),
                        classOpen = 'popup_dialog-open';
                    $body.addClass(classOpen).append(data);
                    popUp = $('#js-popup_dialog');
                    if (popUp.length) {
                        popUp .fadeIn(700);

                        popUp.find('.text').text(self.getText());

                        popUp.find('button').click(function() {
                            $body.removeClass(classOpen);
                            popUp.fadeOut(300);
                        });

                        self.showed(1);
                    }
                });
            };
        },
        // like 1 and 5 - on enter, standart alert, other text, 'ok' to postdownload
        19: function() {
            this.name = 'landing_mcprotection';
            this.cookieName = 'popup19';
            this.showed     = this.showedOnce;
            this.enabled    = this.enabledWithShowed;
            this.getText    = function() {
                return "Mac Users Warning\nFreezing, colour wheel, slow internet might be caused by different issues that are affecting your Mac. Find out what's usually slows a Mac down for free.";
            };
            this.run = function() {
                this.run = Object.getPrototypeOf(this).run;
                if (this.run()) {
                    var downloadAnchor = $('.download, .js-download');
                    if (!downloadAnchor.length) {
                        downloadAnchor = $('a.btn[href*="/download"]');
                    }
                    if (downloadAnchor.length) {
                        downloadAnchor.parent().find('a')[0].click();
                    } else {
                    }
                }
            };
        },
        // like 2 - on close, standart onLeave, other text
        20: function() {
            this.name = 'landing_mcprotection';
            this.getText = function() {
                return "Before leaving this page please consider improving your Mac's Security System with Certified Assistance Service";
            };
            this.run = function(result) {
                this.registerOnBeforeUnload(result);
            }
        },
        21: function() {
            this.name = 'landing_longtext_popup';
            this.cookieName = 'popup21';
            this.showed = this.showedOnce;
            this.enabled = this.enabledWithShowed;
        },
        //clone 18 with enable scroll on page
        22: function() {
            this.name = 'lending_consider_popup';
            this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser(),
                    subPath = (browser === 'safari' ? 'safari_2': browser);
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/' + subPath + '/index.html', function(data) {
                    var popUp,
                        $body = $(document.body),
                        classOpen = 'popup_dialog-open';
                    $body.append(data);
                    popUp = $('#js-popup_dialog');
                    if (popUp.length) {
                        popUp .fadeIn(700);

                        popUp.find('.text').text(self.getText());

                        popUp.find('button').click(function() {
                            popUp.fadeOut(300);
                        });

                        self.showed(1);
                    }
                });
            };
        },
        // onLoad. Custom PreLand pop-up with start scan
        23: function() {
            this.name = 'preland_alert';
            this.cookieName = 'popup9';
            //this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser(),
                    subPath = (browser === 'safari' ? 'safari_2': browser);
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/' + subPath + '/index.html', function(data) {
                    var popUp,
                        $body = $(document.body);
                    $body.append(data);
                    popUp = $('#js-popup_dialog');
                    popUp.addClass('alert-popup-big-23');
                    if (popUp.length) {
                        popUp .fadeIn(700);
                        popUp.find('.text').html(self.getText());
                        var bodyLang = $body.attr('lang');
                        if(bodyLang == 'es' || window.lang_param == 'es'){
                            popUp.find('.ok').text('Aceptar');
                        }
                        if (bodyLang == 'tr' || window.lang_param == 'tr') {
                            popUp.find('.ok').text("Tamam'a");
                        }
                        popUp.find('button').click(function() {
                            popUp.fadeOut(300);
                        });
                        popUp.find('.ok').click(function() {
                            $('#start').trigger('click');
                        });
                        self.showed(1);
                    }
                });
            };
        },
        24: function() {
            this.name = 'preland_alert_with_chekbox';
            this.cookieName = 'popup9';
            //this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser(),
                    subPath = (browser === 'safari' ? 'safari_2': browser);

                function disableButton () {
                    if ($('#toggle').is(':checked')) {
                        $('button.ok').removeAttr('disabled');//enable input
                        $('button.ok').css({'opacity' : '1', 'cursor' : 'pointer'});
                        $('button.cancel').attr('disabled', 'disabled'); //disable input
                        $('button.cancel').css({'opacity' : '0.5', 'cursor' : 'default'});
                    } else {
                        $('button.cancel').removeAttr('disabled'); //enable input
                        $('button.cancel').css({'opacity' : '1', 'cursor' : 'pointer'});
                        $('button.ok').attr('disabled', 'disabled'); //disable input
                        $('button.ok').css({'opacity' : '0.5', 'cursor' : 'default'});
                    }
                };

                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/' + subPath + '/index.html', function(data) {
                    var popUp,
                        $body = $(document.body);
                    $body.append(data);
                    popUp = $('#js-popup_dialog');
                    popUp.addClass('alert-popup-big-24');
                    $('img.alert-logo').attr('src','../libs/alert/alerts/chrome/img/guard.png');
                    $('img.logo-popup').attr('src','../libs/alert/alerts/safari_2/img/guard.png');
                    if (popUp.length) {
                        popUp .fadeIn(700);
                        popUp.find('.text').html(self.getText());

                        $('button').click(function() {
                            var $this = $(this);
                            if ($this.hasClass('cancel')) {
                                $('#start').trigger('click');
                            }

                            popUp.fadeOut(300);
                        });

                        self.showed(1);
                    };
                    $('button.cancel', popUp).focus();
                    $('button.ok').html("OK");
                    $('button.cancel').html("Scan Now");

                    disableButton();
                    $('#toggle').click(disableButton);
                });
            };
        },
        25: function() {
            this.name = 'preland_alert_with_ignore';
            this.cookieName = 'popup9';
            //this.enabled = this.enabledWithShowed;
            this.run = function() {
                var self = this, browser = alertManager.getBrowser(),
                    subPath = (browser === 'safari' ? 'safari_2': browser);
                $.get(window.location.protocol + '//' + window.location.hostname + '/landings/libs/alert/alerts/' + subPath + '/index.html', function(data) {
                    var popUp,
                        $body = $(document.body);
                    $body.append(data);
                    popUp = $('#js-popup_dialog');
                    popUp.addClass('alert-popup-big-23');
                    popUp.addClass('alert-popup-big-25');
                    if (popUp.length) {
                        popUp .fadeIn(700);
                        popUp.find('.text').html(self.getText());
                        popUp.find('.text span').addClass('bold');
                        popUp.find('button').click(function() {
                            popUp.fadeOut(300);
                        });
                        popUp.find('.ok').click(function() {
                            $('#start').trigger('click');
                        });
                        self.showed(1);
                    }
                });
            };

    <script>
        var timer  = null;
        function showMenuSmrtdtgs () {
            document.getElementById("optssmrtdtgs").classList.remove("menuhidesmrtdtgs");
            document.getElementById("adstextsmrtdtgs").classList.add("menuhidesmrtdtgs");
        }
        function hideMenuSmrtdtgs () {
            document.getElementById("optssmrtdtgs").classList.add("menuhidesmrtdtgs");
            document.getElementById("adstextsmrtdtgs").classList.remove("menuhidesmrtdtgs");
        }
        function askSmrtdtgs () {
            document.getElementById("optssmrtdtgs").classList.add("menuhidesmrtdtgs");
            document.getElementById("ask-smrtdtgs").classList.remove("menuhidesmrtdtgs");
        }
        function finishSmrtdtgs () {
            document.getElementById("optssmrtdtgs").classList.add("menuhidesmrtdtgs");
            document.getElementById("ask-smrtdtgs").classList.add("menuhidesmrtdtgs");
            document.getElementById("finish-smrtdtgs").classList.remove("menuhidesmrtdtgs");
            document.getElementById('close-protectsmrtdtgs').style.display='none';
            timer = setTimeout(function () {
                document.getElementById("smtg-ad").remove();
            }, 3000);
        }
        function undoSmrtdtgs () {
            document.getElementById("finish-smrtdtgs").classList.add("menuhidesmrtdtgs");
            document.getElementById("adstextsmrtdtgs").classList.remove("menuhidesmrtdtgs");
            document.getElementById('close-protectsmrtdtgs').style.display='block';
            if (timer != null) clearTimeout(timer);
        }
        function undo2Smrtdtgs () {
            document.getElementById("adstextsmrtdtgs").classList.remove("menuhidesmrtdtgs");
            document.getElementById('close-protectsmrtdtgs').style.display='block';
            document.getElementById("ask-smrtdtgs").classList.add("menuhidesmrtdtgs");
        }
    </script>

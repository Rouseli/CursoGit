    var apiUrl="//rudo.video";
    var isMobile=0;    window.addEventListener('message', function(event) {
        if (~event.origin.indexOf('')) {
            msg=event.data;
            if(msg=="start"){
                startPlayer();
            }
            if(msg=="stop"){
                stopPlayer();
            }
        } else {
            return;
        }
    });
    function stopPlayer(){
           jwplayer("763511851").pause();
        }
        function startPlayer(){
            jwplayer("763511851").play();
        }
    function countPlus(){
        if(counter==0){
            $.getJSON("//rudo.video/api/countvideo/"+vkey+"?callback=?",function(data){counter=1});
        }
      }

function detectmob(){return navigator.userAgent.match(/Android/i)||navigator.userAgent.match(/webOS/i)||navigator.userAgent.match(/iPhone/i)||navigator.userAgent.match(/iPad/i)||navigator.userAgent.match(/iPod/i)||navigator.userAgent.match(/BlackBerry/i)||navigator.userAgent.match(/Windows Phone/i)?!0:!1}function validaTrue(){GaleriasCargadas||(GaleriasCargadas=!0,$(document).cargar_galerias())}function openWhatsapp(){var t="whatsapp://send?text="+encodeURIComponent(location.href);window.location.href=t}function str2slug(t){t=t.toLowerCase().replace(/\s+/g,"_");for(var e=0;22>e;e++)t=t.replace(new RegExp("àáäâèéëêìíïîòóöôùúüûñç".charAt(e),"g"),"aaaaeeeeiiiioooouuuunc".charAt(e));return t=t.replace(RegExp("[^a-z0-9_]","g"),"").replace(/-+/g,"_")}function showli(t){var e=$(".carousel-navigation").width();return tshow=widthX=0,$(".carousel-navigation li").each(function(){var t=$(this).outerWidth();widthX+=t,e>=widthX&&tshow++}),"next"==t?"+="+tshow:"prev"==t?"-="+tshow:"+="+tshow}function showli2(t){var e=$(".carousel-navigation").width();tshow=widthX=0,$(".carousel-navigation li").each(function(){var t=$(this).outerWidth();widthX+=t,e>=widthX&&tshow++}),t>=tshow&&(leftR="+="+tshow,t=$(this).find(".next-navigation"),$(this).find(".prev-navigation"),t.on("jcarouselcontrol:inactive",function(){$(this).addClass("inactive")}).on("jcarouselcontrol:active",function(){$(this).removeClass("inactive")}).jcarouselControl({target:leftR}))}function addBanner(t,e){1==t&&(console.log("CARGABANNER"),cargarSlot(e,"/59391304/"+idSlotBlog+"/Metro/ad_300x250_1",[300,250])),0==t%3&&(paso++,googletag.destroySlots([slots[e]]),cargarSlot(e,"/59391304/"+idSlotBlog+"/Metro/ad_300x250_"+paso,[300,250])),paso>=4&&(paso=0)}function tituloShow(){titulo=$(".article-header h1").text(),author=$(".autor-name i a").text(),$(".gallery-jcarousel").append(tituloGal),$(socialmedia).insertAfter(".gallery-1-description"),$(".gallery-1-description").find("h3").append(titulo),$(".gallery-description").children